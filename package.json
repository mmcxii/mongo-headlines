{
    "name": "mongo-headlines",
    "version": "1.0.0",
    "description": "A web scraper that saves headlines in mongo db",
    "main": "server.js",
    "dependencies": {
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "mongoose": "^5.7.1",
        "ts-node": "^8.4.1",
        "ts-mongoose": "0.0.19"
    },
    "devDependencies": {
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/mongoose": "^5.5.17",
        "@types/node": "^12.7.7",
        "concurrently": "^4.1.2",
        "nodemon": "^1.19.2",
        "typescript": "^3.6.3"
    },
    "scripts": {
        "start": "ts-node src/server.ts",
        "server": "nodemon -x ts-node src/server.ts",
        "client": "npm start --prefix client",
        "client-install": "npm install --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mmcxii/mongo-headlines.git"
    },
    "author": "Nich Secord",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mmcxii/mongo-headlines/issues"
    }
}
